{
  "name": "joplin-plugin-knowledge-graph",
  "version": "1.0.5",
  "scripts": {
    "dist": "webpack --joplin-plugin-config buildMain && webpack --joplin-plugin-config buildExtraScripts && webpack --joplin-plugin-config createArchive",
    "dev": "webpack --mode development --joplin-plugin-config buildMain && webpack --joplin-plugin-config buildExtraScripts && webpack --joplin-plugin-config createArchive",
    "prepare": "npm run dist",
    "update": "npm install -g generator-joplin && yo joplin --update",
    "test": "jest --watch",
    "test-ui": "webpack serve --config ./test/ui/webpack.config.js --mode development",
    "generate-graph": "tsc --project test/tsconfig.json && ts-node ./test/tools/run-generate-graph"
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin",
    "graph"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.15.0",
    "@types/d3": "^7.1.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.178",
    "@types/mdast": "^3.0.10",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^3.0.3",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "jest": "^27.2.5",
    "on-build-webpack": "^0.1.0",
    "style-loader": "^2.0.0",
    "tar": "^6.0.5",
    "ts-jest": "^27.0.7",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^4.3.1",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.6",
    "css-loader": "^5.2.7",
    "d3": "^7.1.1",
    "d3-force": "^3.0.0",
    "force-graph": "^1.42.4",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.6",
    "react": "^17.0.2",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^17.0.2",
    "react-sliding-side-panel": "^2.0.3",
    "react-tag-input": "^6.7.3",
    "rehype-stringify": "^9.0.2",
